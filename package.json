{
  "name": "generator-ts-pkg",
  "version": "0.0.1",
  "description": "Scaffold your typescript project with esbuild and commitlint",
  "homepage": "https://github.com/rawnly/yo-ts-pkg",
  "author": {
    "name": "Federico Vitale",
    "email": "mail@fedevitale.dev",
    "url": "https://fedevitale.dev"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "typescript",
    "ts",
    "javascript",
    "node",
    "package",
    "module",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "rawnly/generator-ts-pkg",
  "license": "MIT"
}
